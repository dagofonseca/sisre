<p id="notice"><%= notice %></p>

<h1>Solicituds</h1>

<table>
  <thead>
    <tr>
      <th>Nombredelevento</th>
      <th>Numerodeasistentes</th>
      <th>Descripcion</th>
      <th>Estado</th>
      <th>Computadores</th>
      <th>Videobeam</th>
      <th>Comentarios</th>
      <th>Expedicion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @solicituds.each do |solicitud| %>
      <tr>
        <td><%= solicitud.nombredelevento %></td>
        <td><%= solicitud.numeroDeAsistentes %></td>
        <td><%= solicitud.descripcion %></td>
        <td><%= solicitud.estado %></td>
        <td><%= solicitud.computadores %></td>
        <td><%= solicitud.videoBeam %></td>
        <td><%= solicitud.comentarios %></td>
        <td><%= solicitud.expedicion %></td>
        <td><%= link_to 'Show', solicitud %></td>
        <td><%= link_to 'Edit', edit_solicitud_path(solicitud) %></td>
        <td><%= link_to 'Destroy', solicitud, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Solicitud', new_solicitud_path %>
